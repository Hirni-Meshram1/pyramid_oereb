<%page args="map"/>
%if map.image:
<Image>${map.image.encode()}</Image>
%endif
%if map.reference_wms:
<ReferenceWMS>${map.reference_wms|u}</ReferenceWMS>
%endif
%if map.legend_at_web:
<LegendAtWeb>${map.legend_at_web|u}</LegendAtWeb>
%endif
%if map.legends:
<OtherLegend>
%for legend_entry in map.legends:
<%include file="legend_entry.xml" args="legend_entry=legend_entry"/>
%endfor
</OtherLegend>
%endif
